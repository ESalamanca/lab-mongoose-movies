<h2>List of all the Celebrities</h2>
<ul>
{{#each celebrities}}
<li>
  <a href="/celebrities/{{_id}}">{{name}}</a>
  <form action="/celebrities/{{_id}}/delete" method="POST">
  <button type="submit">Delete</button>
  </form>
  <form action="/celebrities/{{_id}}/edit" method="GET">
  <button type="submit">Edit</button>
  </form>
</li>
{{/each}}
</ul>
<a class="add-new" href='/celebrities/new'>Add</a>
